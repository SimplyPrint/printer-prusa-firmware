+#include "screen_sp_print_result.hpp"
+#include "marlin_client.hpp"
+#include "ScreenHandler.hpp"
+#include <window_msgbox.hpp>
+#include <connect/marlin_printer.hpp>
+#include <connect/connect.hpp>
+#include "img_resources.hpp"
+
+SPPrintResultScreen::SPPrintResultScreen(window_t *parent)
+        : ScreenPrintingModel(_(caption)),
+          bed_icon(this,
+                   Rect16((display::GetW() - img::serial_printing_172x138.w) / 2, GuiDefaults::RectScreenBody.Top(),
+                          img::serial_printing_172x138.w, img::serial_printing_172x138.h),
+                   &img::serial_printing_172x138) {
+
+
+    SetButtonIconAndLabel(BtnSocket::Middle, BtnRes::Disconnect, LabelRes::Stop);
+    DisableButton(BtnSocket::Middle);
+    parent_window = parent;
+    bed_icon.Disable();
+    bed_icon.Unshadow();
+    setupComponents();
+}
+
+void SPPrintResultScreen::setupComponents() {
+    SetButtonIconAndLabel(BtnSocket::Middle, BtnRes::Bed_Clear, LabelRes::Bed_Clear);
+}
+
+void SPPrintResultScreen::Show() {
+    ScreenPrintingModel::Show();
+    bed_icon.Show();
+}
+
+void SPPrintResultScreen::Hide() {
+    bed_icon.Hide();
+    ScreenPrintingModel::Hide();
+}
+
+void SPPrintResultScreen::tuneAction() {
+    // Action to clear the bed when the print is completed
+    SharedBuffer buffer;
+    connect_client::MarlinPrinter connect_printer;
+    connect_client::Connect client(connect_printer, buffer);
+    int response_code = client.send_simplyprint("print succeeded");
+    if (response_code / 100 != 2) {
+        MsgBoxWarning(_("Failed to send print result message."), Responses_Ok);
+    }
+    void * param = nullptr;
+    parent_window->WindowEvent(this, GUI_event_t::CHILD_CHANGED, param);
+
+}
+
+void SPPrintResultScreen::stopAction() {
+    // Action to clear the bed when the print is completed
+    connect_client::MarlinPrinter connect_printer;
+    SharedBuffer buffer;
+    connect_client::Connect client(connect_printer, buffer);
+    int response_code = client.send_simplyprint("print failed");
+    if (response_code / 100 != 2) {
+        MsgBoxWarning(_("Failed to send print result message."), Responses_Ok);
+    }
+    void * param = nullptr;
+    parent_window->WindowEvent(this, GUI_event_t::CHILD_CHANGED, param);
+}
+
+void SPPrintResultScreen::pauseAction() {}
+
+void SPPrintResultScreen::windowEvent(window_t *, GUI_event_t event, void * param) {
+    if (event == GUI_event_t::ENC_UP || event == GUI_event_t::CLICK || event == GUI_event_t::TOUCH_CLICK) {
+//        if (GetParent()) {
+//            GetParent()->WindowEvent(this, GUI_event_t::CHILD_CHANGED, param);
+//        }
+    }
+    ScreenPrintingModel::windowEvent(this, event, param);
+}
